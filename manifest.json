{
  "manifest_version": 2,
  "name": "WebSite Helper",
  "version": "1.1",
  "description": "Util function for private",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab","storage"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ]
    }
  ]
}
